@model Filminurk.Models.UserComments.UserCommentsIndexViewModel

<h1>Vaata kommentaari</h1>
<div>
	<h4>Info:</h4>
	<hr />
    <dl class="row">
		<div class="form-group row">
			<label asp-for="CommentID" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentID)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CommentUserID" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentUserID)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CommentBody" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentBody)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CommentScore" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentScore)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="IsHelpful" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.IsHelpful)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="IsHarmful" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.IsHarmful)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CommentCreatedAt" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentCreatedAt)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CommentModifiedAt" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentModifiedAt)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CommentDeletedAt" class="col-sm 2 col-form-label"></label>
			@Html.DisplayFor(model => model.CommentDeletedAt)
		</div>
	</dl>
	<hr />
	<a asp-action="Index" asp-controller="UserComments" class="btn btn-primary">Tagasi</a>
</div>
